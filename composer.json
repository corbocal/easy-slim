{
    "name": "corbocal/easy-slim",
    "description": "A lightweight package for a quickstart with slim-php",
    "type": "library",
    "license": "GNU GPL v3.0",
    "version": "1.0.0",
    "authors": [
        {
            "name": "corbocal",
            "email": "monkey-factory@protonmail.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "monolog/monolog": "^3.9",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0",
        "slim/http": "^1.4",
        "slim/slim": "^4.14"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "phpstan/phpstan": "^2.0",
        "squizlabs/php_codesniffer": "^3.10",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Corbocal\\EasySlim\\": "src/",
            "Corbocal\\EasySlim\\Tests\\": "tests/"
        }
    }
}